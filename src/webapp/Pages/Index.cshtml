@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>


<div>
    @foreach( var forecast in @Model.TheWeather)
    {
        <div>
        Date: @Html.DisplayFor( modelItem => forecast.Date)
        Temperature: @Html.DisplayFor( modelItem => forecast.TemperatureF)
        Summary : @Html.DisplayFor( modelItem => forecast.Summary)

        </div>

    }
</div>